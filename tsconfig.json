{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "jsx": "react",
        "noImplicitAny": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}