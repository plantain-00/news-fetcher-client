  "main": "main.js",
  "scripts": {
    "osx": "rm -rf dist/news-darwin-x64 && electron-packager . 'news' --out=dist --arch=x64 --version=0.37.6 --app-version='1.0.6' --platform=darwin --ignore='dist/'",
    "win": "rm -rf dist/news-win32-x64 && electron-packager . 'news' --out=dist --arch=x64 --version=0.37.6 --app-version='1.0.6' --platform=win32 --ignore='dist/'",
