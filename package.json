{
  "name": "news-fetcher-client",
  "version": "1.2.3",
  "description": "The client side of a cross-platform tool to get and sync news.",
  "main": "main.js",
  "scripts": {
    "lint": "tslint \"scripts/*.tsx\" \"*.ts\" && npm run standard",
    "build": "tsc && tsc -p static && npm run clean && npm run cpy && npm run cleancss && npm run webpack",
    "start": "electron .",
    "clean": "rimraf static/index.bundle.* static/css static/fonts",
    "webpack": "webpack --config static/webpack.config.js",
    "cleancss": "cleancss -o static/index.bundle.css static/index.css",
    "test": "tsc -p spec && jasmine && tsc -p static_spec && karma start static_spec/karma.config.js",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "release": "rimraf dist && clean-release",
    "cpy": "cpy ./node_modules/bootstrap/dist/css/bootstrap.min.css static/css/ && cpy ./node_modules/bootstrap/fonts/*.* ./static/fonts && cpy ./node_modules/highlight.js/styles/github.css static/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/news-fetcher-client.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/news-fetcher-client/issues"
  },
  "homepage": "https://github.com/plantain-00/news-fetcher-client#readme",
  "dependencies": {
    "cheerio": "0.22.0",
    "request": "2.81.0",
    "tslib": "1.7.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.1",
    "@types/highlight.js": "9.1.9",
    "@types/jasmine": "2.5.53",
    "@types/react": "15.0.34",
    "@types/react-dom": "15.5.1",
    "@types/request": "0.0.45",
    "bootstrap": "3.3.7",
    "clean-css-cli": "4.1.6",
    "clean-release": "1.2.0",
    "cpy-cli": "1.0.1",
    "electron": "1.6.11",
    "highlight.js": "9.12.0",
    "jasmine": "2.6.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-webpack": "2.0.3",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "rimraf": "2.6.1",
    "schema-based-json-editor": "4.0.1",
    "standard": "10.0.2",
    "tslint": "5.4.3",
    "typescript": "2.4.1",
    "webpack": "3.2.0"
  }
}
