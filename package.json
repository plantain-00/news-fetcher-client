  "dependencies": {
    "bootstrap": "^3.3.6",
    "cheerio": "^0.20.0",
    "jquery": "^2.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "request": "^2.67.0"
  },
  "engines": {
    "node": ">=6.0.0"
