{
  "name": "news-fetcher-client",
  "version": "1.0.7",
  "description": "",
  "main": "main.js",
  "scripts": {
    "osx": "rm -rf distribution/news-darwin-x64 && electron-packager . 'news' --out=distribution --arch=x64 --version=1.1.0 --app-version='1.0.7' --platform=darwin --ignore='distribution/'",
    "win": "rm -rf distribution/news-win32-x64 && electron-packager . 'news' --out=distribution --arch=x64 --version=1.1.0 --app-version='1.0.7' --platform=win32 --ignore='distribution/'",
    "tslint": "tslint \"scripts/*.tsx\" \"*.ts\"",
    "check": "npm-check -u",
    "init": "npm i -g typescript tsd tslint npm-check electron-prebuilt electron-packager",
    "restore": "tsd install && npm i"
  },
  "repository": {
    "type": "git",
